PRE=$1
 cd experiments/

 python create_experiment_params.py --mod_order qpsk --protocol private_preamble --mod1 neural --mod2 neural --model_params_template $(pwd)/../custom_model_params/${PRE}_model_params.libsonnet --train_snr_db low --num_trials 24 --batch_size 256 --experiment_name QPSK_test_low_${PRE} --num_results_logged 30
 python create_experiment_params.py --mod_order qpsk --protocol private_preamble --mod1 neural --mod2 neural --model_params_template $(pwd)/../custom_model_params/${PRE}_model_params.libsonnet --train_snr_db mid --num_trials 24 --batch_size 256 --experiment_name QPSK_test_mid_${PRE} --num_results_logged 30
 python create_experiment_params.py --mod_order qpsk --protocol private_preamble --mod1 neural --mod2 neural --model_params_template $(pwd)/../custom_model_params/${PRE}_model_params.libsonnet --train_snr_db high --num_trials 24 --batch_size 256 --experiment_name QPSK_test_high_${PRE} --num_results_logged 30

 python make_jobs.py --experiment_folder private_preamble/QPSK_test_low_${PRE}
 python make_jobs.py --experiment_folder private_preamble/QPSK_test_mid_${PRE}
 python make_jobs.py --experiment_folder private_preamble/QPSK_test_high_${PRE}

 cd ..

 ./runecho_args -b -n 1 -j experiments/private_preamble/QPSK_test_low_${PRE}
 ./runecho_args -b -n 1 -j experiments/private_preamble/QPSK_test_mid_${PRE}
 ./runecho_args -b -n 1 -j experiments/private_preamble/QPSK_test_high_${PRE}